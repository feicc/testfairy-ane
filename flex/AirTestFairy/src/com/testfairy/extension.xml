<extension xmlns="http://ns.adobe.com/air/extension/3.1">
    <id>com.testfairy</id>
    <versionNumber>0.0.1</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAirTestFairy.a</nativeLibrary>
                <initializer>AirTestFairyInitializer</initializer>
                <finalizer>AirTestFairyFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>libAirTestFairy.jar</nativeLibrary>
                <initializer>com.testfairy.AirTestFairy</initializer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>   
	</platforms>
</extension>
